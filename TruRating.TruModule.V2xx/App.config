<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <appSettings>
    <add key="Endpoint" value="https://tru-sand-service-v2xx.trurating.com/api/servicemessage,http://localhost:31415/api/servicemessage,http://localhost:47851/api/servicemessage"/>
    <!--
    
    Register an account in the developer portal to get a PartnerId, MerchantId and TransportKey.  You can make up your own TerminalId
    
    Uncomment the following elements and enter your own values.
    
    If TerminalId is omitted, the local machine name will be used.
    -->
    <!--<add key="TerminalId" value="{your_terminal_id}"/>-->
    <!--<add key="MerchantId" value="{your_merchant_id}"/>-->
    <!--<add key="PartnerId" value="{your_partner_id}"/>-->
    <!--<add key="TransportKey" value="{your_transport_key}"/>-->
    <!---->
    <add key="Verbose" value="true"/>
    <add key="Languages" value="en-GB"/>
    
    <!-- Possible values are V200 V210 V220-->
    <add key="TsiVersion" value="V220"/>
    
