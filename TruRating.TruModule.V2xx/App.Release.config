<?xml version="1.0" encoding="utf-8" ?>
<!-- For more information on using transformations 
     see the web.config examples at http://go.microsoft.com/fwlink/?LinkId=214134. -->
<configuration xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">
  <appSettings xdt:Transform="Replace">
    <add key="Endpoint" value="https://tru-sand-service-v200.trurating.com/api/servicemessage"/>
    <!--
    
    Register an account in the developer portal to get a PartnerId, MerchantId and TransportKey.  You can make up your own TerminalId
    
    Uncomment the following elements and enter your own values.
    
    If TerminalId is omitted, the local machine name will be used.
    -->
    <!--<add key="TerminalId" value="{your_terminal_id}"/>-->
    <!--<add key="MerchantId" value="{your_merchant_id}"/>-->
    <!--<add key="PartnerId" value="{your_partner_id}"/>-->
    <!--<add key="TransportKey" value="{your_transport_key}"/>-->
    <!---->
    <add key="Verbose" value="true"/>
    <add key="Languages" value="en-GB"/>
    
    <!-- Possible values are V200 V210 V220-->
    <add key="TsiVersion" value="V210"/>
    
    <!-- Possible values are None Events EventList -->
    <add key="PosIntegration" value="None"/>
    <!-- Automatic will continuously collect ratings-->
    <add key="Automatic" value="false"/>
    <!-- Setting this to true will cause TruModule to send this code instead of hardcoded form data.-->
    <add key="RegistrationCode" value="false"/>
  </appSettings>
</configuration>
